
public class pattern17 {

//	* * * * * 
//	* * * * * 
//	* * * * * 
//	* * * * * 
//	* * * * * 

	// it,s important to next pattern so understand this pattern.

	public static void main(String[] args) {

		for (int i = 0; i < 5; i++) {

			for (int j = 0; j < 5; j++) {

				System.out.print("*" + " ");
			}
			System.err.println();
		}

	}

}
